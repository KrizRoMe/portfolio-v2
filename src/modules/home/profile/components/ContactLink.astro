---
interface Props {
    href: string
    icon: (props: { class: string }) => Element
}

const { href, icon: Icon }: Props = Astro.props
---

<a
    href={href}
    aria-label='Contact link'
    aria-labelledby='contact-link'
    class='text-black/85 hover:text-black dark:text-white/85 dark:hover:text-white'>
    <Icon class='h-5 w-5' />
</a>
